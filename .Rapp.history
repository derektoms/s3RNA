Intersect <- function (x) {  #
  # Multiple set version of intersect#
  # x is a list#
  if (length(x) == 1) {#
    unlist(x)#
  } else if (length(x) == 2) {#
    intersect(x[[1]], x[[2]])#
  } else if (length(x) > 2){#
    intersect(x[[1]], Intersect(x[-1]))#
  }#
}
